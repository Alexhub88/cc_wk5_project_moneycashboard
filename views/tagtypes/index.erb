<h1>List of Tag Types</h1>
<% @tagtypes.each do |tagtype| %>
  Tag Type:  <%= tagtype.type %> <br>
  <br>
    <form method = "post" action = "/tagtypes/<%=tagtype.id%>/delete">
      <input type = "submit" value = "Delete Tag Type"/>
    </form><br>

    <a href="/tagtypes/<%=tagtype.id%>">Show Details</a>
    <hr />
<% end %>
<br>
