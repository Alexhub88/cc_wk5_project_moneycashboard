<h1>Tag Types</h1>
<br>
<a href="/tagtypes/new">New Tag Type</a>
<br>
<br>
<br>
<h2>Tag Type List</h2>
<br>
<table class="trans-table">
  <tr>
    <th>Tag Type</th>
    <th>Show</th>
    <th>Delete</th>
    </tr>
<% @tagtypes.each do |tagtype| %>
  <tr>
    <td><a><%= tagtype.type %></a></td>
    <td><form method = "get" action = "/tagtypes/<%=tagtype.id%>">
    <input type = "submit" value = "Show Tag Type"/></form></td>
    <td><form method = "post" action = "/tagtypes/<%=tagtype.id%>/delete">
    <input type = "submit" value = "Delete Tag Type"/></form></td>
  </tr>
<% end %>
<table>
